{
    "name": "extly/foo",
    "description": "Lorem ipsum dolor sit amet, consectetur adipiscing elit",
    "authors": [
        {
            "name": "Extly CB",
            "email": "team@extly.com"
        }
    ],
    "support": {
        "issues": "https://support.extly.com/",
        "docs": "https://www.extly.com/docs/"
    },
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "TheNewNamespace\\": "library/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "library/tests/"
        }
    },
    "config": {
        "github-protocols": [
            "https"
        ],
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3.33"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "library/vendor"
    },
    "extra": {
        "config": {
            "platform": {
                "php": "7.3.33"
            }
        },
        "php-prefixer": {
            "global-scope-prefix": "XT_",
            "namespaces-prefix": "XT_BUILD",
            "project-name": "Foo Name"
        }
    }
}